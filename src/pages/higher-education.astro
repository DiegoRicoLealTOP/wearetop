---
import Layout from "@/layouts/Layout.astro";
import Hero from "@/components/higher-education/hero.astro";
import Info from "@/components/higher-education/info.astro";
import CountDown from "@/components/higher-education/countdown.astro";
import Bento from "@/components/higher-education/bento.astro";
import ScrollDrivenAnimation from "@/components/higher-education/scroll-driven-animation.astro";
import Pricing from "@/components/higher-education/pricing.astro";
import Contact from "@/components/higher-education/contact.svelte";
---

<Layout
  title="Higher Education - WeAreTop"
  description="Explore our higher education programs abroad. Gain international experience and advance your career with WeAreTop."
>
  <Hero />
  <ScrollDrivenAnimation />
  <CountDown />
  <Pricing />
  <Bento />
  <Info />
  <Contact client:visible />
</Layout>
